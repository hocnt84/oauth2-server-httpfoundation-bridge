    "keywords":["oauth","oauth2","auth","httpfoundation"],
    "type":"library",
    "license":"MIT",
    "authors":[
        {
            "name":"Brent Shaffer",
            "email": "bshafs@gmail.com",
            "homepage":"http://brentertainment.com"
        }
    ],
    "homepage": "http://github.com/bshaffer/oauth2-server-httpfoundation-bridge",
    "repositories": [
        {
            "type": "vcs",
